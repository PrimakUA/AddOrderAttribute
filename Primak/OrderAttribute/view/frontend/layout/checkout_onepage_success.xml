<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="order.success.additional.info">
            <block class="Magento\Checkout\Block\Onepage\Success" name="primak.order.success"
                template="Primak_OrderAttribute::order/success.phtml" after="-">
                <arguments>
                    <argument name="viewModelPrimakAttribute" xsi:type="object">Primak\OrderAttribute\ViewModel\PrimakAttribute</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
